{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",

  "rushVersion": "5.71.0",
  "pnpmVersion": "6.7.1",

  "pnpmOptions": {
    "strictPeerDependencies": true,
    "useWorkspaces": false
  },

  "nodeSupportedVersionRange": ">=12.13.0 <13.0.0 || >=14.15.0 <15.0.0 || >=16.13.0 <17.0.0",
  "ensureConsistentVersions": false,

  "repository": {
    "url": "https://github.com/matt-winfield/atomic-redux",
    "defaultBranch": "master",
    "defaultRemote": "origin"
  },

  "eventHooks": {
    "preRushInstall": [
    ],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },

  "variants": [
  ],

  "projects": [
    {
      "packageName": "atomic-redux-state",
      "projectFolder": "atomic-redux-state",
      "reviewCategory": "production",
      "shouldPublish": true
    },

    {
      "packageName": "atomic-redux-state-react",
      "projectFolder": "atomic-redux-state-react",
      "reviewCategory": "production",
      "shouldPublish": true
    },
    
    {
      "packageName": "atomic-redux-example",
      "projectFolder": "examples/atomic-redux-example",
      "reviewCategory": "tools"
    },

    // {
    //   "packageName": "server-side-rendering",
    //   "projectFolder": "examples/server-side-rendering",
    //   "reviewCategory": "tools"
    // }
  ]
}
